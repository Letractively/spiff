<?xml version="1.0" ?>
<xml>
  <package handle="spiff_core_extension_manager">
    <name>Extension Manager</name>
    <release>0.1</release>
    <description xml:lang="" lang="">
    This core extension implements the user interface for installing and
    removing extensions.
    </description>
    <authors>
      <person>
        <name>Samuel Abels</name>
        <email>spam2, debain, org</email>
      </person>
    </authors>

    <behavior>
      <signal uri="render_start" />
      <signal uri="render_end" />
    </behavior>

    <depends>
      <runtime>spiff</runtime>
      <installtime>spiff</installtime>
    </depends>
  </package>
</xml>
