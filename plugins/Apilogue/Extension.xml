<?xml version="1.0" ?>
<xml>
  <package handle="spiff_apilogue">
    <name>Apilogue</name>
    <release>0.1</release>
    <description xml:lang="" lang="">
    Parse API documentation embedded in source code, edit it online
    in a Wiki, and export it back to the source.
    </description>
    <authors>
      <person>
        <name>Samuel Abels</name>
        <email>spam2, debain, org</email>
      </person>
    </authors>

    <behavior>
      <signal uri="render_start" />
      <signal uri="render_end" />
    </behavior>

    <depends>
      <runtime>spiff</runtime>
      <installtime>spiff</installtime>
    </depends>

    <attributes>
      <attribute name="recursive" type="boolean">True</attribute>
    </attributes>
  </package>
</xml>
