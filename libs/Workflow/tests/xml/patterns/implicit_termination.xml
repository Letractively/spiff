<?xml version="1.0" encoding="UTF-8"?> 
<process-definition name="flow" revision="1.0">
    <description>Pattern 11 (Implicit Termination)</description>

    <!-- Start with an implicit simple split. -->
    <startactivity>
        <successor>first</successor>
    </startactivity>
    
    <activity name="first">
        <successor>activity_f1</successor>
        <successor>activity_f2</successor>
        <successor>activity_f3</successor>
    </activity>

    <stubactivity name="activity_f1" />
    <stubactivity name="activity_f2" />
    <stubactivity name="activity_f3" />

    <!-- Implicit end. -->
</process-definition>
