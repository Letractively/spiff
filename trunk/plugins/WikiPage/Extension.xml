<?xml version="1.0" ?>
<xml>
  <extension handle="spiff_core_wiki_page">
    <name>Wiki Page</name>
    <release>0.1</release>
    <description xml:lang="" lang="">
    This core extension shows normal web pages written in plain text
    with some annotations.
    </description>
    <authors>
      <person>
        <name>Samuel Abels</name>
        <email>spam2, debain, org</email>
      </person>
    </authors>

    <behavior>
      <signal uri="render_start" />
      <signal uri="render_end" />
    </behavior>

    <depends>
      <runtime>spiff</runtime>
      <installtime>spiff</installtime>
    </depends>

    <attributes>
      <attribute name="recursive" type="boolean">True</attribute>
    </attributes>
  </extension>
</xml>
